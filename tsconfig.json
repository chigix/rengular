{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@rengular/simulation": [
        "projects/simulation/src/public-api"
      ],
      "@rengular/ren-schema-reg": [
        "projects/schema-reg/src/public-api"
      ],
      "@rengular-component/primitive": [
        "projects/primitive/src/public-api"
      ],
      "@rengular-component/prototypes": [
        "projects/prototypes/src/public-api"
      ],
      "@rengular-component/prototypes/rdf-schema": [
        "projects/prototypes/src/rdf-schema"
      ],
      "@rengular/network-context": [
        "projects/network-context/src/public-api"
      ],
      "@rengular-component/gekijo": [
        "projects/gekijo/src/public-api"
      ],
      "@rengular-component/gekijo/rdf-schema": [
        "projects/gekijo/src/rdf-schema"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
